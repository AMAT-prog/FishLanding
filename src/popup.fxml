<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/18">
   
<BorderPane fx:id="viewFisherHistory_popup" prefHeight="429.0" prefWidth="594.0">
    <center>
        <TabPane fx:id="history_tabpane">
            <tabs>
                <Tab text="Catches">
                    <TableView fx:id="catch_tv">
                        <columns>
                            <TableColumn fx:id="catchDate_col" text="Date" />
                            <TableColumn fx:id="species_col" text="Species" />
                            <TableColumn fx:id="quantity_col" text="Quantity" />
                            <TableColumn fx:id="price_col" text="Price/kg" />
                            <TableColumn fx:id="total_col" text="Total" />
                        </columns>
                    </TableView>
                </Tab>
                <Tab text="Transactions">
                    <TableView fx:id="txn_tv">
                        <columns>
                            <TableColumn fx:id="buyer_col" text="Buyer" />
                            <TableColumn fx:id="qtySold_col" text="Qty Sold" />
                            <TableColumn fx:id="unitPrice_col" text="Unit Price" />
                            <TableColumn fx:id="totalPrice_col" text="Total Price" />
                            <TableColumn fx:id="status_col" text="Status" />
                        </columns>
                    </TableView>
                </Tab>
                <Tab text="Dock Logs">
                    <TableView fx:id="dock_tv">
                        <columns>
                            <TableColumn fx:id="dockDate_col" text="Date" />
                            <TableColumn fx:id="arrival_col" text="Arrival" />
                            <TableColumn fx:id="departure_col" text="Departure" />
                            <TableColumn fx:id="remarks_col" text="Remarks" />
                        </columns>
                    </TableView>
                </Tab>
            </tabs>
        </TabPane>
    </center>
</BorderPane>


</AnchorPane>
